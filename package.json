{
  "name": "api-template",
  "version": "0.1",
  "description": "",
  "main": "",
  "scripts": {
    "update-packages": "sh ./scripts/update-packages.sh",
    "prepare": "husky install",
    "psql": "docker exec -it -postgres psql -U dev dbname",
    "api:dev": "npx nodemon source/api.ts | pino-pretty --translateTime",
    "compose:up": "docker compose -f ./docker-compose.dev.yml up -d",
    "compose:start": "docker compose -f ./docker-compose.dev.yml start",
    "compose:stop": "docker compose -f ./docker-compose.dev.yml stop",
    "compose:down": "docker compose -f ./docker-compose.dev.yml down --volumes",
    "dev:restart": "npm run dev:down && npm run dev:up && sleep 2 && npm run migrate",
    "migrate:new": "npx knex migrate:make --knexfile ./database/knexfile.ts --migrations-directory ../migrations",
    "migrate": "npx knex migrate:latest --knexfile ./database/knexfile.ts --migrations-directory ../migrations"
  },
  "author": "Winter4 <mywinter4@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.18.0",
    "ioredis": "^5.4.1",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "pino": "^9.0.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "npm-check-updates": "^16.14.20",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
